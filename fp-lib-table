# kicad footprint library table
# should be linked to ~/fp-lib-table
# mc 2/27/2014

(fp_lib_table
  (lib (name analog-devices)(type KiCad)(uri ${KISYSMOD}/analog-devices.pretty)(options "")(descr "footprints for unique parts from Analog Devices"))
  (lib (name asm)(type KiCad)(uri ${KISYSMOD}/asm.pretty)(options "")(descr "assembly and mechanical footprints"))
  (lib (name bluegiga)(type KiCad)(uri ${KISYSMOD}/bluegiga.pretty)(options "")(descr "footprints for unique parts from Bluegiga"))
  (lib (name bourns)(type KiCad)(uri ${KISYSMOD}/bourns.pretty)(options "")(descr "footprints for unique parts from Bourns"))
  (lib (name connect-sm)(type KiCad)(uri ${KISYSMOD}/connect-sm.pretty)(options "")(descr "surface mount connectors, plugs, and jacks"))
  (lib (name connect-thru)(type KiCad)(uri ${KISYSMOD}/connect-thru.pretty)(options "")(descr "through hole connectors, plugs, and jacks"))

  (lib (name connect-thru_leg)(type Legacy)(uri ${KISYSMOD}/connect_thru.mod)(options "")(descr "migrating"))


)
